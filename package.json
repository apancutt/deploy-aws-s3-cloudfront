{
  "name": "deploy-aws-s3-cloudfront",
  "version": "3.0.0-beta.0",
  "description": "Syncs a local directory to an AWS S3 bucket, optionally invalidating affected CloudFront paths.",
  "keywords": [
    "aws",
    "s3",
    "cloudfront",
    "deploy"
  ],
  "homepage": "https://github.com/apancutt/deploy-aws-s3-cloudfront#readme",
  "bugs": {
    "url": "https://github.com/apancutt/deploy-aws-s3-cloudfront/issues"
  },
  "author": "Adam Pancutt <adam@pancutt.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apancutt/deploy-aws-s3-cloudfront.git"
  },
  "bin": "./bin/deploy-aws-s3-cloudfront.js",
  "files": [
    "bin",
    "src"
  ],
  "dependencies": {
    "aws-sdk": "2",
    "fast-glob": "3",
    "md5-file": "5",
    "micromatch": "4",
    "mime-types": "2",
    "prompt-confirm": "2",
    "winston": "3",
    "yargs": "15"
  },
  "devDependencies": {
    "babel-eslint": "10",
    "eslint": "6",
    "jest": "25"
  },
  "scripts": {
    "eslint": "eslint */**/*.js",
    "test": "jest"
  },
  "eslintConfig": {
    "env": {
      "browser": false,
      "commonjs": true,
      "es6": true,
      "jest": true,
      "node": true
    },
    "parser": "babel-eslint",
    "extends": "eslint:recommended"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/__tests__/mock/",
      "/node_modules/"
    ]
  }
}
